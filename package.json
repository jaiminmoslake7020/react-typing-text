    "@vanilla-extract/css": "^1.16.0",
    "@vanilla-extract/recipes": "^0.5.5",
    "@vanilla-extract/rollup-plugin": "^1.3.10",
    "@vanilla-extract/vite-plugin": "^4.0.16",
    "@vitejs/plugin-react": "^4.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.22.4",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.7.0",
    "tsx": "^4.19.1",
