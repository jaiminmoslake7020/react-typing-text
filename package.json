{
  "name": "react-typing-text",
  "version": "0.0.1",
  "description": "It allows to have typing text effect for a text string in react web application.",
  "directories": {
    "test": "test"
  },
  "type": "module",  
  "files": ["dist"],
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
